{
  "name": "js-template",
  "version": "1.0.0",
  "description": "",
  "scripts": {
    "build": "./bin/build.sh",
    "check-updates": "npx npm-check-updates",
    "start-ts": "tsc --noEmit --watch",
    "start-parcel": "parcel index.html data/**/* -p 1234 --no-hmr",
    "start": "run-p start-ts start-parcel",
    "lint": "eslint '*/**/*.{js,ts,tsx}' --quiet",
    "ts": "tsc --noEmit",
    "test": "env NODE_ENV=test jest",
    "test-all": "run-s lint ts test"
  },
  "alias": {
    "src": "./src"
  },
  "author": "Greg Tatum",
  "license": "MIT",
  "browserslist": [
    "since 2019-01"
  ],
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.{js,jsx,ts,tsx}",
      "!**/node_modules/**"
    ],
    "testEnvironment": "jsdom",
    "moduleFileExtensions": [
      "js",
      "jsx",
      "ts",
      "tsx"
    ],
    "moduleDirectories": [
      "node_modules"
    ],
    "moduleNameMapper": {
      "\\.(css|less)$": "<rootDir>/src/test/__mocks__/style.ts"
    },
    "roots": [
      "src/test"
    ],
    "globalSetup": "./src/test/setup.ts",
    "globalTeardown": "./src/test/teardown.ts",
    "verbose": true
  },
  "devDependencies": {
    "@babel/core": "^7.17.5",
    "@babel/preset-env": "^7.16.11",
    "@babel/preset-react": "^7.16.7",
    "@babel/preset-typescript": "^7.16.7",
    "@testing-library/react": "^12.1.3",
    "@types/common-tags": "^1.8.1",
    "@types/jest": "^27.4.1",
    "@types/react": "^17.0.39",
    "@types/react-dom": "^17.0.11",
    "@typescript-eslint/eslint-plugin": "^5.12.1",
    "@typescript-eslint/parser": "^5.12.1",
    "babel-jest": "^28.0.2",
    "babel-plugin-module-resolver": "^4.1.0",
    "common-tags": "^1.8.2",
    "eslint": "^8.9.0",
    "eslint-config-prettier": "^8.4.0",
    "eslint-plugin-import": "^2.25.4",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-react": "^7.29.0",
    "jest": "^27.5.1",
    "npm-run-all": "^4.1.5",
    "parcel": "^2.5.0",
    "parcel-plugin-wasm.rs": "^1.3.0",
    "parcel-resolver-ignore": "^2.0.0",
    "prettier": "^2.5.1",
    "typescript": "^4.5.5"
  },
  "dependencies": {
    "dropbox": "^10.28.0",
    "nosleep.js": "^0.12.0",
    "pdfjs-dist": "^2.13.216",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-redux": "^7.2.6",
    "react-router-dom": "^6.3.0",
    "redux": "^4.2.0",
    "redux-thunk": "^2.4.1"
  }
}
